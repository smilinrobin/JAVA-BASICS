public class printstar{
	public static void main (String args[]){
		int m = 0;
		for(int i=1;i<=4;i++){
			for(int j=1;j<=4-i;j++){
				System.out.print("j");}

			for (int n=0;n<=i+m;n++){
							if (n%2==0){
								System.out.print("#");}
							else {System.out.print("n");}
		}
		m = m+1;
		System.out.println("");
		}
			}
		}
